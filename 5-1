<body>
    <script> 
       function inDayFibonacci(n) {
            let fib = [0, 1];
            for (let i = 2; i < n; i++) {
                fib[i] = fib[i - 1] + fib[i - 2];
            }
            document.write("DÃ£y Fibonacci: " + fib.join(", ") + "<br>");
            }
        inDayFibonacci(20);

    </script>
</body>
