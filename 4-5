<body>
    <script> 
       function tongFibonacci(n) {
            let fib = [0, 1];
            let sum = 1;
            for (let i = 2; i < n; i++) {
                fib[i] = fib[i - 1] + fib[i - 2];
                sum += fib[i];
        }
        return sum;
        }
        document.write("Tổng của 20 số Fibonacci đầu tiên: " + tongFibonacci(20));
    </script>
</body>
