<body>
    <script> 
       function fibonacci(n) {
            let fib = [0, 1];
        for (let i = 2; i < n; i++) {
            fib[i] = fib[i - 1] + fib[i - 2];
            }
        return fib;
            }
            document.write("20 số Fibonacci đầu tiên: " + fibonacci(20).join(", "));
    </script>
</body>
</html>
