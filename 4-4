<body>
    <script> 
       function soFibonacciChiaHetCho5() {
            let a = 0, b = 1;
            while (true) {
                let c = a + b;
                if (c % 5 === 0) return c;
                a = b;
                b = c;
            }
        }
        document.write("Số Fibonacci đầu tiên chia hết cho 5: " + soFibonacciChiaHetCho5());
    </script>
</body>
